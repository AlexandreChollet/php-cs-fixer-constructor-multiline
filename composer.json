{
    "name": "alexandrechollet/php-cs-fixer-constructor-multiline",
    "description": "PHP CS Fixer rule to enforce multiline constructor arguments",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Chollet",
            "email": "alexandre.chollet85@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "friendsofphp/php-cs-fixer": "^3.75"
    },
    "autoload": {
        "psr-4": {
            "AlexandreChollet\\PhpCsFixerConstructorMultiline\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexandreChollet\\PhpCsFixerConstructorMultiline\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
